{
  "name": "silinternational/simplesamlphp-module-mfa",
  "description": "A simpleSAMLphp module for prompting the user for MFA credentials (such as a TOTP code, etc.).",
  "type": "simplesamlphp-module",
  "license": "LGPL-2.1-or-later",
  "authors": [
    {
      "name": "Matt Henderson",
      "email": "matt_henderson@sil.org"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": ">=7.2",
    "ext-json": "*",
    "roave/security-advisories": "dev-master",
    "silinternational/php-env": "^2.1",
    "silinternational/psr3-adapters": "^1.1 || ^2.0",
    "simplesamlphp/simplesamlphp": "~1.18.4",
    "silinternational/idp-id-broker-php-client": "^3.0",
    "sinergi/browser-detector": "^6.1"
  },
  "require-dev": {
    "behat/behat": "^3.3",
    "behat/mink": "^1.7",
    "behat/mink-goutte-driver": "^1.2",
    "phpunit/phpunit": "^8.4"
  },
  "autoload": {
    "psr-4": {
      "Sil\\SspMfa\\": "src/",
      "Sil\\SspMfa\\Behat\\": "features/"
    }
  }
}
